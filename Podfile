# Uncomment the next line to define a global platform for your project
# pod install
# pod update

platform :ios, '9.0'

target 'UIClient' do

  workspace 'UIClient'
  project 'UIClient/UIClient'

  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for UIClient
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'

  pod 'FirebaseAnalytics'
  pod 'FirebaseMessaging'
  
  pod 'AppsFlyerFramework'
  
  pod 'AFNetworking', '3.2.1'
  pod 'FCUUID', '1.3.1'
  
end

target 'VGPSDK' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  workspace 'UIClient'
  project 'VGPSDK/VGPSDK'

  # Pods for VGPSDK
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'

  pod 'FirebaseAnalytics'
  pod 'FirebaseMessaging'
  
  pod 'AppsFlyerFramework'
  
  pod 'AFNetworking', '3.2.1'
  pod 'FCUUID', '1.3.1'
  
end
